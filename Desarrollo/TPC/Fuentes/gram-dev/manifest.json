{
    "manifest_version": 3,
    "name": "silly corrector",
    "version": "1.0",
    "description": "holaa",
    "permissions":[
        "contextMenus",
        "activeTab",
        "scripting",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://mail.google.com/*"],
            "js": ["content.js"]
        }
    ],
    "icons": {
        "48": "tpc.png"
    }
}